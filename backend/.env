# Server Configuration
PORT=3000
NODE_ENV=development

# Database Configuration
MONGODB_URI=mongodb://localhost:27017/multicloud
REDIS_URL=redis://localhost:6379

# JWT Configuration
JWT_SECRET=multicloud_jwt_secret_key_here
JWT_EXPIRES_IN=2h
REFRESH_TOKEN_EXPIRES_IN=7d

# Cloud Provider API Keys (for development only)
ALIYUN_APP_KEY=your_aliyun_app_key
ALIYUN_APP_SECRET=your_aliyun_app_secret
BAIDU_API_KEY=your_baidu_api_key
BAIDU_SECRET_KEY=your_baidu_secret_key
TENCENT_APP_ID=your_tencent_app_id
TENCENT_APP_SECRET=your_tencent_app_secret

# Encryption
ENCRYPTION_KEY=multicloud_encryption_key_32_chars_min

# Logging
LOG_LEVEL=info

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100